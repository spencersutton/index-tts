class TextMessage(GradioModel):
    text: str
    type: Literal["text"] = ...

class TextMessageDict(TypedDict):
    text: str
    type: Literal["text"]

class ComponentMessage(GradioModel):
    component: str
    value: Any
    constructor_args: dict[str, Any]
    props: dict[str, Any]
    type: Literal["component"] = ...

class ComponentMessageDict(TypedDict):
    component: str
    value: Any
    constructor_args: dict[str, Any]
    props: dict[str, Any]
    type: Literal["component"]
    instance: NotRequired[GradioComponent]

type MessageContent = str | FileDataDict | FileData | Component

class MessageDict(TypedDict):
    content: MessageContent | list[MessageContent]
    role: Literal["user", "assistant", "system"]
    metadata: NotRequired[MetadataDict]
    options: NotRequired[list[OptionDict]]

class FileMessage(GradioModel):
    file: FileData
    alt_text: str | None = ...
    type: Literal["file"] = ...

class FileMessageDict(TypedDict):
    file: FileDataDict
    alt_text: NotRequired[str | None]
    type: Literal["file"]

type NormalizedMessageContent = TextMessageDict | FileMessageDict | ComponentMessageDict

class NormalizedMessageDict(TypedDict):
    content: list[NormalizedMessageContent]
    role: Literal["user", "assistant", "system"]
    metadata: NotRequired[MetadataDict]
    options: NotRequired[list[OptionDict]]

class Message(GradioModel):
    role: str
    metadata: MetadataDict | None = ...
    content: list[TextMessage | FileMessage | ComponentMessage]
    options: list[OptionDict] | None = ...

class ExampleMessage(TypedDict):
    icon: NotRequired[str | FileDataDict]
    display_text: NotRequired[str]
    text: NotRequired[str]
    files: NotRequired[Sequence[str | FileDataDict]]

@document()
@dataclass
class ChatMessage:
    content: MessageContent | list[MessageContent]
    role: Literal["user", "assistant", "system"] = ...
    metadata: MetadataDict = ...
    options: list[OptionDict] = ...

class ChatbotDataMessages(GradioRootModel):
    root: list[Message]

