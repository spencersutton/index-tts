from collections import UserDict
from dataclasses import dataclass
from typing import Any, Self, TypeVar

"""Contains a base class for all inference types."""
T = TypeVar("T", bound=BaseInferenceType)

def dataclass_with_extra[T: BaseInferenceType](cls: type[T]) -> type[T]: ...

@dataclass
class BaseInferenceType(UserDict):
    @classmethod
    def parse_obj_as_list(cls, data: bytes | str | list | dict) -> list[Self]: ...
    @classmethod
    def parse_obj_as_instance(cls, data: bytes | str | list | dict) -> Self: ...
    @classmethod
    def parse_obj(cls, data: bytes | str | list | dict) -> list[Self] | Self: ...
    def __post_init__(self):  # -> None:
        ...
    def __setitem__(self, __key: Any, __value: Any) -> None: ...
    def __setattr__(self, __name: str, __value: Any) -> None: ...

def normalize_key(key: str) -> str: ...
