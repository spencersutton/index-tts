class Event:
    def __init__(self, enable_timing: bool = ...) -> None: ...
    def __del__(self) -> None: ...
    def record(self) -> None: ...
    def wait(self) -> None: ...
    def query(self) -> bool: ...
    def synchronize(self) -> None: ...
    def elapsed_time(self, end_event: Event) -> float: ...
