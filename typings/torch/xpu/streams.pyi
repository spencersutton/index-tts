import torch

if not hasattr(torch._C, "_XpuStreamBase"): ...

class Stream(torch._C._XpuStreamBase):
    def __new__(cls, device=..., priority=..., **kwargs) -> Self: ...
    def wait_event(self, event) -> None: ...
    def wait_stream(self, stream) -> None: ...
    def record_event(self, event=...) -> torch.xpu.streams.Event | torch._C.Event: ...
    def query(self) -> bool: ...
    def synchronize(self) -> None: ...
    def __eq__(self, o) -> bool: ...
    def __hash__(self) -> int: ...

class Event(torch._C._XpuEventBase):
    def __new__(cls, enable_timing=...) -> Self: ...
    def record(self, stream=...) -> None: ...
    def wait(self, stream=...) -> None: ...
    def query(self) -> bool: ...
    def elapsed_time(self, end_event) -> float: ...
    def synchronize(self) -> None: ...
